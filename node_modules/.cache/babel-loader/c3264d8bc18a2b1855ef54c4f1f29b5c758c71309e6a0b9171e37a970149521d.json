{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      posts: [],\n      // Í≤åÏãúÍ∏Ä Î™©Î°ù\n      filteredPosts: [],\n      // ÌïÑÌÑ∞ÎßÅÎêú Í≤åÏãúÍ∏Ä Î™©Î°ù\n      loading: true,\n      // Î°úÎî© ÏÉÅÌÉú\n      error: false,\n      // Ïò§Î•ò ÏÉÅÌÉú\n      title: '',\n      // Í≤åÏãúÍ∏Ä Ï†úÎ™©\n      content: '',\n      // Í≤åÏãúÍ∏Ä ÎÇ¥Ïö©\n      currentPage: 'board',\n      // ÌòÑÏû¨ ÌéòÏù¥ÏßÄ ÏÉÅÌÉú ('board', 'write')\n      currentUser: null,\n      // ÌòÑÏû¨ Î°úÍ∑∏Ïù∏Ìïú ÏÇ¨Ïö©Ïûê Ï†ïÎ≥¥\n      sortOrder: 'latest',\n      // Ï†ïÎ†¨ Í∏∞Ï§Ä\n      searchQuery: '',\n      // Í≤ÄÏÉâÏñ¥\n      searchType: 'title',\n      // Í≤ÄÏÉâ Ìï≠Î™© (Ï†úÎ™©, ÎÇ¥Ïö©, Îì±Î°ùÏûêÎ™Ö)\n      image: null // Ï∂îÍ∞Ä\n    };\n  },\n  computed: {\n    // Í≤ÄÏÉâÎêú Í≤åÏãúÍ∏Ä Î™©Î°ùÏùÑ Í∏∞Ï§ÄÏúºÎ°ú Ï†ïÎ†¨Îêú Í≤åÏãúÍ∏ÄÏùÑ Î∞òÌôò\n    sortedPosts() {\n      return [...this.filteredPosts].sort((a, b) => {\n        if (this.sortOrder === 'latest') {\n          return new Date(b.createdAt) - new Date(a.createdAt); // ÏµúÏã†Ïàú\n        } else if (this.sortOrder === 'oldest') {\n          return new Date(a.createdAt) - new Date(b.createdAt); // Ïò§ÎûòÎêúÏàú\n        } else if (this.sortOrder === 'likes') {\n          return (b.likes || 0) - (a.likes || 0); // Ï¢ãÏïÑÏöîÏàú\n        } else if (this.sortOrder === 'views') {\n          return (b.views || 0) - (a.views || 0); // Ï°∞ÌöåÏàòÏàú\n        }\n      });\n    }\n  },\n  created() {\n    this.initData(); // Ï¥àÍ∏∞ Îç∞Ïù¥ÌÑ∞ Î°úÎìú\n    this.checkLoginStatus(); // Î°úÍ∑∏Ïù∏ ÏÉÅÌÉú ÌôïÏù∏\n  },\n  methods: {\n    handleImageUpload(event) {\n      this.image = event.target.files[0];\n    },\n    // Î°úÍ∑∏Ïù∏ ÏÉÅÌÉú ÌôïÏù∏\n    async checkLoginStatus() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_URL}/auth/check-login`, {\n          withCredentials: true\n        });\n        this.currentUser = response.data.user || null; // Î°úÍ∑∏Ïù∏ ÏÇ¨Ïö©Ïûê Ï†ïÎ≥¥\n      } catch (error) {\n        console.error('Î°úÍ∑∏Ïù∏ ÏÉÅÌÉú ÌôïÏù∏ Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);\n        this.currentUser = null; // Î°úÍ∑∏ÏïÑÏõÉ ÏÉÅÌÉú\n      }\n    },\n    // Ï¥àÍ∏∞ Îç∞Ïù¥ÌÑ∞ Î°úÎìú\n    async initData() {\n      this.loading = true;\n      this.error = false;\n      try {\n        await this.fetchPosts();\n      } catch (error) {\n        console.error('Ï¥àÍ∏∞ Îç∞Ïù¥ÌÑ∞Î•º Í∞ÄÏ†∏Ïò§Îäî Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);\n        this.error = true;\n      } finally {\n        this.loading = false;\n      }\n    },\n    // Í≤åÏãúÍ∏Ä Î™©Î°ù Í∞ÄÏ†∏Ïò§Í∏∞\n    async fetchPosts() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_URL}/api/board`, {\n          withCredentials: true\n        });\n        this.posts = response.data;\n        this.filteredPosts = [...this.posts];\n      } catch (error) {\n        console.error('Í≤åÏãúÍ∏ÄÏùÑ Í∞ÄÏ†∏Ïò§Îäî Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);\n        this.error = true;\n      }\n    },\n    // Í≤åÏãúÍ∏Ä Í≤ÄÏÉâ\n    filterPosts() {\n      if (this.searchQuery.trim()) {\n        // Ï†úÎ™© Í≤ÄÏÉâ\n        if (this.searchType === 'title') {\n          this.filteredPosts = this.posts.filter(post => post.title.toLowerCase().includes(this.searchQuery.toLowerCase()));\n        }\n        // ÎÇ¥Ïö© Í≤ÄÏÉâ\n        else if (this.searchType === 'content') {\n          this.filteredPosts = this.posts.filter(post => post.content.toLowerCase().includes(this.searchQuery.toLowerCase()));\n        }\n        // Îì±Î°ùÏûêÎ™Ö Í≤ÄÏÉâ\n        else if (this.searchType === 'author') {\n          this.filteredPosts = this.posts.filter(post => (post.author || '').toLowerCase().includes(this.searchQuery.toLowerCase()));\n        }\n      } else {\n        this.filteredPosts = [...this.posts]; // Í≤ÄÏÉâÏñ¥ ÏóÜÏúºÎ©¥ ÏõêÎ≥∏ Ï†ÑÏ≤¥\n      }\n    },\n    // Í≤åÏãúÍ∏Ä ÏûëÏÑ±\n    async submitPost() {\n      const formData = new FormData();\n      formData.append('title', this.title);\n      formData.append('content', this.content);\n      if (this.image) {\n        formData.append('image', this.image);\n      }\n      try {\n        await axios.post(`${process.env.VUE_APP_API_URL}/api/board`, formData, {\n          withCredentials: true,\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        this.goToBoardPage();\n        this.initData();\n      } catch (error) {\n        console.error('Í≤åÏãúÍ∏Ä ÏûëÏÑ± Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);\n      }\n    },\n    // ÌéòÏù¥ÏßÄ Ïù¥Îèô\n    goToBoardPage() {\n      this.currentPage = 'board';\n      this.title = '';\n      this.content = '';\n    },\n    goToWritePage() {\n      if (!this.currentUser) {\n        alert('Î°úÍ∑∏Ïù∏Ïù¥ ÌïÑÏöîÌï©ÎãàÎã§.');\n        return;\n      }\n      this.currentPage = 'write';\n    },\n    goToDetailPage(postId) {\n      this.$router.push({\n        name: 'BoardDetail',\n        params: {\n          id: postId\n        }\n      });\n    },\n    // ÎÇ†Ïßú ÌòïÏãù Î≥ÄÍ≤Ω\n    formatDate(dateString) {\n      return new Date(dateString).toLocaleDateString(undefined, {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    },\n    // Í≤ÄÏÉâ Ï¥àÍ∏∞Ìôî\n    resetSearch() {\n      this.searchQuery = '';\n      this.filteredPosts = [...this.posts];\n    },\n    // Í∞úÌñâ Î¨∏Ïûê -> <br> Î≥ÄÌôò\n    convertNewLinesToBreaks(text) {\n      return text.replace(/\\n/g, '<br>');\n    }\n  }\n};","map":{"version":3,"names":["axios","data","posts","filteredPosts","loading","error","title","content","currentPage","currentUser","sortOrder","searchQuery","searchType","image","computed","sortedPosts","sort","a","b","Date","createdAt","likes","views","created","initData","checkLoginStatus","methods","handleImageUpload","event","target","files","response","get","process","env","VUE_APP_API_URL","withCredentials","user","console","fetchPosts","filterPosts","trim","filter","post","toLowerCase","includes","author","submitPost","formData","FormData","append","headers","goToBoardPage","goToWritePage","alert","goToDetailPage","postId","$router","push","name","params","id","formatDate","dateString","toLocaleDateString","undefined","year","month","day","hour","minute","resetSearch","convertNewLinesToBreaks","text","replace"],"sources":["/Users/bangguem/Desktop/·ÑÜ·Ö•·Ñå·Öµ/Duo-Mate/src/components/BoardForm.vue"],"sourcesContent":["<template>\n  <div class=\"contents\">\n    <!-- [1] Í≤åÏãúÍ∏Ä Î™©Î°ù(Í≤ÄÏÉâ/Ï†ïÎ†¨/ÏûëÏÑ± Î≤ÑÌäº Ìè¨Ìï®) ÏòÅÏó≠ -->\n    <section class=\"contents-header\" v-if=\"currentPage === 'board'\">\n      <div class=\"header-left\">\n        <!-- Ï†ïÎ†¨ Î∞©Ïãù ÏÑ†ÌÉù -->\n        <div class=\"select-wrapper\">\n          <select id=\"sort\" v-model=\"sortOrder\" @change=\"sortPosts\" class=\"filter-dropdown\">\n            <option value=\"latest\">ÏµúÏã†Ïàú</option>\n            <option value=\"oldest\">Ïò§ÎûòÎêúÏàú</option>\n            <option value=\"likes\">Ï¢ãÏïÑÏöîÏàú</option>\n            <option value=\"views\">Ï°∞ÌöåÏàòÏàú</option>\n          </select>\n        </div>\n\n        <!-- Í≤ÄÏÉâ Ï°∞Í±¥ ÏÑ†ÌÉù -->\n        <div class=\"select-wrapper\">\n          <select v-model=\"searchType\" class=\"filter-dropdown\">\n            <option value=\"title\">Ï†úÎ™©</option>\n            <option value=\"content\">ÎÇ¥Ïö©</option>\n            <option value=\"author\">Îì±Î°ùÏûêÎ™Ö</option>\n          </select>\n        </div>\n\n        <!-- Í≤ÄÏÉâ Ï¥àÍ∏∞Ìôî Î≤ÑÌäº -->\n        <button @click=\"resetSearch\" class=\"reset-button\">Ï¥àÍ∏∞Ìôî</button>\n      </div>\n\n      <div class=\"header-right\">\n        <!-- Í≤ÄÏÉâ Î∞ïÏä§ -->\n        <div class=\"search-box\">\n          <input v-model=\"searchQuery\" @keyup.enter=\"filterPosts\" type=\"text\" placeholder=\"Í≤ÄÏÉâÏñ¥Î•º ÏûÖÎ†•ÌïòÏÑ∏Ïöî\"\n            class=\"search-input\" />\n          <span class=\"search-icon\" @click=\"filterPosts\">üîç</span>\n        </div>\n      </div>\n    </section>\n\n    <!-- Í≤åÏãúÍ∏Ä ÏûëÏÑ± Ïù¥Îèô Î≤ÑÌäº -->\n    <div class=\"write-button-container\" v-if=\"currentPage === 'board'\">\n      <button @click=\"goToWritePage\" class=\"write-button\">Í≤åÏãúÍ∏Ä ÏûëÏÑ±</button>\n    </div>\n\n    <!-- Í≤åÏãúÍ∏Ä Î™©Î°ù -->\n    <div v-if=\"currentPage === 'board'\" class=\"feed-container\">\n      <!-- Î°úÎî©/ÏóêÎü¨/Í≤åÏãúÍ∏Ä Î™©Î°ù ÏÉÅÌÉú ÌëúÏãú -->\n      <div v-if=\"loading\" class=\"loading\">Î°úÎî© Ï§ë...</div>\n      <div v-else-if=\"error\" class=\"error\">Í≤åÏãúÍ∏ÄÏùÑ Î∂àÎü¨Ïò§Îäî Îç∞ Ïã§Ìå®ÌñàÏäµÎãàÎã§.</div>\n\n      <div v-else-if=\"sortedPosts.length\" class=\"feed-list\">\n        <div v-for=\"post in sortedPosts\" :key=\"post._id\" class=\"feed-card\">\n          <div class=\"feed-header\">\n            <strong>{{ post.author || 'ÏûëÏÑ±Ïûê ÏóÜÏùå' }}</strong>\n            <div>{{ formatDate(post.createdAt) }}</div>\n          </div>\n          <h2 class=\"feed-title\" @click=\"goToDetailPage(post._id)\">{{ post.title }}</h2>\n          <p class=\"feed-content\" v-html=\"convertNewLinesToBreaks(post.content)\"></p>\n\n          <div v-if=\"post.imageUrl\" class=\"feed-image\">\n            <img :src=\"`http://localhost:3000${post.imageUrl}`\" alt=\"Í≤åÏãúÍ∏Ä Ïù¥ÎØ∏ÏßÄ\" />\n          </div>\n\n          <div class=\"feed-actions\">\n            <span>üëç {{ post.likes || 0 }}</span>\n            <span style=\"margin-left: 10px;\">üëé {{ post.dislikes || 0 }}</span>\n            <span class=\"view-count\" style=\"margin-left: 10px;\">Ï°∞ÌöåÏàò: {{ post.views || 0 }}</span>\n          </div>\n        </div>\n      </div>\n\n      <div v-else class=\"no-posts\">Í≤åÏãúÍ∏ÄÏù¥ ÏóÜÏäµÎãàÎã§.</div>\n    </div>\n\n    <!-- Í≤åÏãúÍ∏Ä ÏûëÏÑ± Ìèº -->\n    <div v-if=\"currentPage === 'write'\" class=\"post-form\">\n      <h2 class=\"post-title\">Í≤åÏãúÍ∏Ä ÏûëÏÑ±</h2>\n      <form @submit.prevent=\"submitPost\">\n        <input v-model=\"title\" type=\"text\" placeholder=\"Ï†úÎ™©ÏùÑ ÏûÖÎ†•ÌïòÏÑ∏Ïöî\" class=\"post-input\" required />\n\n        <textarea v-model=\"content\" placeholder=\"ÎÇ¥Ïö©ÏùÑ ÏûÖÎ†•ÌïòÏÑ∏Ïöî\" class=\"post-textarea\" required></textarea>\n\n        <input type=\"file\" @change=\"handleImageUpload\" accept=\"image/*\" />\n\n        <div class=\"post-buttons\">\n          <button type=\"submit\" class=\"post-submit\">Í≤åÏãúÍ∏Ä ÏûëÏÑ±</button>\n          <button type=\"button\" @click=\"goToBoardPage\" class=\"post-cancel\">Ï∑®ÏÜå</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      posts: [],          // Í≤åÏãúÍ∏Ä Î™©Î°ù\n      filteredPosts: [],  // ÌïÑÌÑ∞ÎßÅÎêú Í≤åÏãúÍ∏Ä Î™©Î°ù\n      loading: true,      // Î°úÎî© ÏÉÅÌÉú\n      error: false,       // Ïò§Î•ò ÏÉÅÌÉú\n      title: '',          // Í≤åÏãúÍ∏Ä Ï†úÎ™©\n      content: '',        // Í≤åÏãúÍ∏Ä ÎÇ¥Ïö©\n      currentPage: 'board',   // ÌòÑÏû¨ ÌéòÏù¥ÏßÄ ÏÉÅÌÉú ('board', 'write')\n      currentUser: null,      // ÌòÑÏû¨ Î°úÍ∑∏Ïù∏Ìïú ÏÇ¨Ïö©Ïûê Ï†ïÎ≥¥\n      sortOrder: 'latest',    // Ï†ïÎ†¨ Í∏∞Ï§Ä\n      searchQuery: '',        // Í≤ÄÏÉâÏñ¥\n      searchType: 'title',    // Í≤ÄÏÉâ Ìï≠Î™© (Ï†úÎ™©, ÎÇ¥Ïö©, Îì±Î°ùÏûêÎ™Ö)\n      image: null, // Ï∂îÍ∞Ä\n    };\n  },\n  computed: {\n    // Í≤ÄÏÉâÎêú Í≤åÏãúÍ∏Ä Î™©Î°ùÏùÑ Í∏∞Ï§ÄÏúºÎ°ú Ï†ïÎ†¨Îêú Í≤åÏãúÍ∏ÄÏùÑ Î∞òÌôò\n    sortedPosts() {\n      return [...this.filteredPosts].sort((a, b) => {\n        if (this.sortOrder === 'latest') {\n          return new Date(b.createdAt) - new Date(a.createdAt); // ÏµúÏã†Ïàú\n        } else if (this.sortOrder === 'oldest') {\n          return new Date(a.createdAt) - new Date(b.createdAt); // Ïò§ÎûòÎêúÏàú\n        } else if (this.sortOrder === 'likes') {\n          return (b.likes || 0) - (a.likes || 0);               // Ï¢ãÏïÑÏöîÏàú\n        } else if (this.sortOrder === 'views') {\n          return (b.views || 0) - (a.views || 0);               // Ï°∞ÌöåÏàòÏàú\n        }\n      });\n    }\n  },\n  created() {\n    this.initData();        // Ï¥àÍ∏∞ Îç∞Ïù¥ÌÑ∞ Î°úÎìú\n    this.checkLoginStatus(); // Î°úÍ∑∏Ïù∏ ÏÉÅÌÉú ÌôïÏù∏\n  },\n  methods: {\n    handleImageUpload(event) {\n      this.image = event.target.files[0];\n    },\n\n    // Î°úÍ∑∏Ïù∏ ÏÉÅÌÉú ÌôïÏù∏\n    async checkLoginStatus() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_URL}/auth/check-login`, {\n          withCredentials: true\n        });\n        this.currentUser = response.data.user || null; // Î°úÍ∑∏Ïù∏ ÏÇ¨Ïö©Ïûê Ï†ïÎ≥¥\n      } catch (error) {\n        console.error('Î°úÍ∑∏Ïù∏ ÏÉÅÌÉú ÌôïÏù∏ Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);\n        this.currentUser = null; // Î°úÍ∑∏ÏïÑÏõÉ ÏÉÅÌÉú\n      }\n    },\n\n    // Ï¥àÍ∏∞ Îç∞Ïù¥ÌÑ∞ Î°úÎìú\n    async initData() {\n      this.loading = true;\n      this.error = false;\n      try {\n        await this.fetchPosts();\n      } catch (error) {\n        console.error('Ï¥àÍ∏∞ Îç∞Ïù¥ÌÑ∞Î•º Í∞ÄÏ†∏Ïò§Îäî Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);\n        this.error = true;\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    // Í≤åÏãúÍ∏Ä Î™©Î°ù Í∞ÄÏ†∏Ïò§Í∏∞\n    async fetchPosts() {\n      try {\n        const response = await axios.get(`${process.env.VUE_APP_API_URL}/api/board`, {\n          withCredentials: true\n        });\n        this.posts = response.data;\n        this.filteredPosts = [...this.posts];\n      } catch (error) {\n        console.error('Í≤åÏãúÍ∏ÄÏùÑ Í∞ÄÏ†∏Ïò§Îäî Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);\n        this.error = true;\n      }\n    },\n\n    // Í≤åÏãúÍ∏Ä Í≤ÄÏÉâ\n    filterPosts() {\n      if (this.searchQuery.trim()) {\n        // Ï†úÎ™© Í≤ÄÏÉâ\n        if (this.searchType === 'title') {\n          this.filteredPosts = this.posts.filter(post =>\n            post.title.toLowerCase().includes(this.searchQuery.toLowerCase())\n          );\n        }\n        // ÎÇ¥Ïö© Í≤ÄÏÉâ\n        else if (this.searchType === 'content') {\n          this.filteredPosts = this.posts.filter(post =>\n            post.content.toLowerCase().includes(this.searchQuery.toLowerCase())\n          );\n        }\n        // Îì±Î°ùÏûêÎ™Ö Í≤ÄÏÉâ\n        else if (this.searchType === 'author') {\n          this.filteredPosts = this.posts.filter(post =>\n            (post.author || '').toLowerCase().includes(this.searchQuery.toLowerCase())\n          );\n        }\n      } else {\n        this.filteredPosts = [...this.posts]; // Í≤ÄÏÉâÏñ¥ ÏóÜÏúºÎ©¥ ÏõêÎ≥∏ Ï†ÑÏ≤¥\n      }\n    },\n\n    // Í≤åÏãúÍ∏Ä ÏûëÏÑ±\n    async submitPost() {\n      const formData = new FormData();\n      formData.append('title', this.title);\n      formData.append('content', this.content);\n      if (this.image) {\n        formData.append('image', this.image);\n      }\n\n      try {\n        await axios.post(`${process.env.VUE_APP_API_URL}/api/board`, formData, {\n          withCredentials: true,\n          headers: {\n            'Content-Type': 'multipart/form-data',\n          },\n        });\n        this.goToBoardPage();\n        this.initData();\n      } catch (error) {\n        console.error('Í≤åÏãúÍ∏Ä ÏûëÏÑ± Ï§ë Ïò§Î•ò Î∞úÏÉù:', error);\n      }\n    },\n\n    // ÌéòÏù¥ÏßÄ Ïù¥Îèô\n    goToBoardPage() {\n      this.currentPage = 'board';\n      this.title = '';\n      this.content = '';\n    },\n    goToWritePage() {\n      if (!this.currentUser) {\n        alert('Î°úÍ∑∏Ïù∏Ïù¥ ÌïÑÏöîÌï©ÎãàÎã§.');\n        return;\n      }\n      this.currentPage = 'write';\n    },\n    goToDetailPage(postId) {\n      this.$router.push({ name: 'BoardDetail', params: { id: postId } });\n    },\n\n    // ÎÇ†Ïßú ÌòïÏãù Î≥ÄÍ≤Ω\n    formatDate(dateString) {\n      return new Date(dateString).toLocaleDateString(undefined, {\n        year: 'numeric',\n        month: 'short',\n        day: 'numeric',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    },\n\n    // Í≤ÄÏÉâ Ï¥àÍ∏∞Ìôî\n    resetSearch() {\n      