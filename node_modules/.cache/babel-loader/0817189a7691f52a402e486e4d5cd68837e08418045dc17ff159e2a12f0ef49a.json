"error\">ÏóÖÎç∞Ïù¥Ìä∏Î•º Î∂àÎü¨Ïò§Îäî Îç∞ Ïã§Ìå®ÌñàÏäµÎãàÎã§.</div>\n      <div v-else-if=\"filteredUpdates.length\" class=\"feed-list\">\n        <div v-for=\"update in filteredUpdates\" :key=\"update._id\" class=\"feed-card\">\n          <div class=\"feed-header\">\n            <img src=\"@/assets/icon_setting.png\" alt=\"ÏóÖÎç∞Ïù¥Ìä∏ ÏïÑÏù¥ÏΩò\" class=\"patch-icon\" />\n            <div class=\"patch-info\">\n              <router-link :to=\"{ name: 'UpdateDetail', params: { id: update._id } }\" class=\"patch-title\">\n                {{ update.title }}\n              </router-link>\n              <p class=\"patch-date\">{{ formatDate(update.date) }}</p>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div v-else class=\"no-updates\">ÏóÖÎç∞Ïù¥Ìä∏Í∞Ä ÏóÜÏäµÎãàÎã§.</div>\n    </div>\n\n    <!-- ÏóÖÎç∞Ïù¥Ìä∏ ÏûëÏÑ± Ìèº -->\n    <div v-if=\"currentPage === 'write'\" class=\"update-form\">\n      <h2>ÏóÖÎç∞Ïù¥Ìä∏ ÏûëÏÑ±</h2>\n      <form @submit.prevent=\"submitUpdate\">\n        <input v-model=\"title\" type=\"text\" placeholder=\"Ï†úÎ™© ÏûÖÎ†•\" required />\n        <textarea v-model=\"content\" placeholder=\"ÏóÖÎç∞Ïù¥Ìä∏ ÎÇ¥Ïö© ÏûÖÎ†•\" required></textarea>\n        <div class=\"form-buttons\">\n          <button type=\"submit\" class=\"submit-button\">ÏûëÏÑ±</button>\n          <button type=\"button\" @click=\"goToListPage\" class=\"cancel-button\">Ï∑®ÏÜå</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      updates: [],\n      loading: false,\n      error: false,\n      currentPage: 'list',\n      sortOrder: 'latest',\n      searchQuery: '',\n      title: '',\n      content: '',\n      currentUser: null // ‚úÖ Î°úÍ∑∏Ïù∏ ÏÇ¨Ïö©Ïûê Ï†ïÎ≥¥\n    };\n  },\n  computed: {\n    // Í≤ÄÏÉâ ÌïÑÌÑ∞ÎßÅ\n    filteredUpdates() {\n      if (!this.searchQuery.trim()) return this.updates;\n      return this.updates.filter(update => {\n       